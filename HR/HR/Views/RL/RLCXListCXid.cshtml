                    <td colspan="3" class="TD_STYLE2">
                        {{dz}}
                    </td>
                    <td class="TD_STYLE1">
                        邮编
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        {{yb1}}
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        国籍
                    </td>
                    <td class="TD_STYLE2">
                       {{gj}}
                    </td>
                    <td class="TD_STYLE1">
                        出生地
                    </td>
                    <td class="TD_STYLE2">
                        {{csd}}
                    </td>
                    <td class="TD_STYLE1">
                        生日
